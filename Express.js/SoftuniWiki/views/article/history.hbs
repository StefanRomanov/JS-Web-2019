
<section>
    <h2>{{articleTitle}}</h2>
    <div class="spacer">
        <h3>Edit history</h3>
        <ul>
            {{#each edits}}
            <li>
                <a href="/edit/{{this._id}}">{{this.creationDate}}‎</a> by
                <span class="author">{{this.author.email}}</span>
            </li>
            {{/each}}
        </ul>
    </div>
</section>